<?xml version="1.0" encoding="UTF-8"?>
<AMA_ConnectivityLayerRQ xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:noNamespaceSchemaLocation="../input_xsd/AMA_ConnectivityLayerRQ.xsd">
    <Requests>
        <Request>
            <target>FlightSearch</target>
            <id>REQ003A</id>
            <action>Search</action>
            <property>
                <key>origin</key>
                <value>JFK</value>
            </property>
            <property>
                <key>destination</key>
                <value>CDG</value>
            </property>
            <property>
                <key>departure_date</key>
                <value>2024-03-15</value>
            </property>
            <set>
                <search_criteria>
                    <cabin_preference>Business</cabin_preference>
                    <max_stops>1</max_stops>
                    <preferred_airlines>
                        <airline>AF</airline>
                        <airline>DL</airline>
                    </preferred_airlines>
                </search_criteria>
            </set>
        </Request>
    </Requests>
    <Requests>
        <Request>
            <target>CarRental</target>
            <id>REQ003B</id>
            <action>Reserve</action>
            <property>
                <key>pickup_location</key>
                <value>CDG</value>
            </property>
            <property>
                <key>dropoff_location</key>
                <value>CDG</value>
            </property>
            <property>
                <key>pickup_date</key>
                <value>2024-03-15</value>
            </property>
            <property>
                <key>dropoff_date</key>
                <value>2024-03-22</value>
            </property>
            <set>
                <rental_details>
                    <car_category>Economy</car_category>
                    <transmission>Automatic</transmission>
                    <fuel_type>Petrol</fuel_type>
                    <driver_age>35</driver_age>
                </rental_details>
                <driver>
                    <name>Michael Brown</name>
                    <license_number>DL123456789</license_number>
                    <license_country>US</license_country>
                </driver>
            </set>
        </Request>
    </Requests>
</AMA_ConnectivityLayerRQ>